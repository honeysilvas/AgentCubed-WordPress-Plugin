<?xml version="1.0" encoding="utf-8"?>
<xsd:schema
  xmlns:xsd="http://www.w3.org/2001/XMLSchema"
  xmlns="http://dataexchange.agentcubed.com"
  xmlns:mstns="http://dataexchange.agentcubed.com"
  elementFormDefault="qualified"
  targetNamespace="http://dataexchange.agentcubed.com"
  version="2.0"
    >
  <xsd:element name="AgentCubedAPI">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="LoginCredentials"
                     type="LoginCredentialsType" />
        <xsd:element name="Leads"
                     type="LeadsType">
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <!-- LoginCredentialsType -->
  <xsd:complexType name="LoginCredentialsType">
    <xsd:sequence>
      <xsd:element name="ErrorNotificationEmail"
                   type="validationEmail">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            This field is required.  It will allow the API to send a notification email back to the address provided in the event an error occurs.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Username"
                   type="string255">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Group_WebLead_Id, user name and password are key elements to determine where a lead is coming from and where it should be assigned.  All of these fields are provided by AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Password"
                   type="string255">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Group_WebLead_Id, user name and password are key elements to determine where a lead is coming from and where it should be assigned.  All of these fields are provided by AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Group_WebLead_ID"
                   type="xsd:int">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Group_WebLead_Id, user name and password are key elements to determine where a lead is coming from and where it should be assigned.  All of these fields are provided by AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LeadSourceKey"
                   type="validationGuidString">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            This field is required and will be provided by your AgentCubed administrator.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>

      <xsd:element name="AgentID"
                   type="validationGuidString"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            AgentID is used when you want to send a record to an exclusive user or an individual agent.
            Pass xsi:nil="true" as default when this feature is not being used, otherwise use the unique identifier that is provided by AgentCubed.
            For example, 6F9619FF-8B86-D011-B42D-00C04FC964FF is a valid uniqueidentifier value, but would fail because it's not mapped to any users.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>

      <xsd:element name="AgentIDType"
                   type="enumAgentIDTypes"
                   minOccurs="0">
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- LeadsType -->
  <xsd:complexType name="LeadsType">
    <xsd:sequence maxOccurs="20">
      <xsd:element name="Lead">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element name="LeadInformation"
                         type="LeadInformationType" />
            <xsd:element name="LeadIndividuals">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  1. Must have at least one Individual with a relationship type of Applicant
                  2. Can not have more than one Individual with a relationship type of Applicant
                  3. Can not have more than one Individual with a relationship type of Spouse
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence maxOccurs="unbounded">
                  <xsd:element name="Individual"
                               type="IndividualDetailsType" />
                </xsd:sequence>
              </xsd:complexType>
              <xsd:unique name="unique1">
                <xsd:selector xpath=".//mstns:Individual"/>
                <xsd:field xpath="@IndividualID"/>
              </xsd:unique>
            </xsd:element>
            <xsd:element name="LeadOpportunities"
                         type="LeadOpportunitiesType" />
            <xsd:element name="LeadContactDetails"
                         type="LeadContactDetailsType" />
            <xsd:element name="LeadVehicleLocations"
                         type="LeadVehicleLocationsType"
                         minOccurs="0" />
            <xsd:element name="LeadVehicles"
                         type="LeadVehiclesType"
                         minOccurs="0" />
            <xsd:element name="LeadBoats"
                         type="LeadBoatsType"
                         minOccurs="0" />
            <xsd:element name="LeadHomes"
                         type="LeadHomesType"
                         minOccurs="0" />
            <xsd:element name="LeadSubsidies"
                         type="LeadSubsidiesType"
                         minOccurs="0" />
            <xsd:element name="LeadInsuranceClaims"
                         type="LeadInsuranceClaimsType"
                         minOccurs="0">
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
        <xsd:key name="key1">
          <xsd:selector xpath=".//mstns:Individual" />
          <xsd:field xpath="@IndividualID" />
        </xsd:key>
        <xsd:keyref name="keyref1"
                    refer="key1">
          <xsd:selector xpath=".//mstns:Driver" />
          <xsd:field xpath="@IndividualIDRef" />
        </xsd:keyref>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- LeadInformationType -->
  <xsd:complexType name="LeadInformationType">
    <xsd:all>
      <xsd:element name="LeadGeneratedDateTime"
                   type="xsd:dateTime">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            The format for the field is yyyy-MM-ddTHH:mm:sszzz or yyyy-MM-ddTHH:mm:ss.
            Example:2011-09-28T12:36:45.4775601-07:00
            The LeadGeneratedDateTime date time should be translated to the Mountain Time zone; however AgentCubed can translate it for you if you let us know what time zone you will be posting it as.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ReferenceKey"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            This is the equivalent of the unique identifier on your end.  It gives the ability to track a lead to your system when needed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ReferenceKeyType"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            This is where you can send a short description of the type of lead (Exclusive, Non-Exclusive, Shared 1-3, Health, Life, etc ).
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Comments"
                   type="xsd:string"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Put additional notes about the lead here.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="LeadOrigin"
                   type="string250"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass the lead origination information here.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="ReferralEmail"
                   type="validationEmail"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Track the originating referral email address here.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey01"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey02"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey03"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey04"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey05"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey06"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey07"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey08"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey09"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="TrackingKey10"
                   type="string50"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            Pass additional information you would like AgentCubed to maintain with the current lead.  This is typically used to house unique identifiers from your system to AgentCubed.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="OriginalDNIS"
                   type="string10"
                   minOccurs="0" />


      <xsd:element name="AdditionalContent01"
                   type="string50"
                   minOccurs="0" />
      <xsd:element name="AdditionalContent02"
                   type="string50"
                   minOccurs="0" />
      <xsd:element name="AdditionalContent03"
                   type="string50"
                   minOccurs="0" />
      <xsd:element name="AdditionalContent04"
                   type="string50"
                   minOccurs="0" />
      <xsd:element name="AdditionalContent05"
                   type="string50"
                   minOccurs="0" />

    </xsd:all>
  </xsd:complexType>

  <!-- IndividualDetailsType -->
  <xsd:complexType name="IndividualDetailsType">
    <xsd:all>
      <xsd:element name="RelationType">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Applicant" />
            <xsd:enumeration value="Spouse" />
            <xsd:enumeration value="Child" />
            <xsd:enumeration value="Parent" />
            <xsd:enumeration value="Grandchild" />
            <xsd:enumeration value="Other" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="SSN"
                   type="string50"
                   minOccurs="0" />
      <xsd:element name="Email"
                   type="validationEmail"
                   minOccurs="0" />
      <xsd:element name="Prefix"
                   type="string8"
                   minOccurs="0" />
      <xsd:element name="FirstName"
                   type="string50"
                   minOccurs="0" />
      <xsd:element name="MiddleName"
                   type="string20"
                   minOccurs="0" />
      <xsd:element name="LastName"
                   type="string50"
                   minOccurs="0" />
      <xsd:element name="Suffix"
                   type="string4"
                   minOccurs="0" />
      <xsd:element name="DOB"
                   type="xsd:date"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            The format for the field is yyyy-MM-dd.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Gender"
                   minOccurs="0">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Male" />
            <xsd:enumeration value="M" />
            <xsd:enumeration value="Female" />
            <xsd:enumeration value="F" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="HeightFT"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            valid values are 0 through 9.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:int">
            <xsd:enumeration value="0" />
            <xsd:enumeration value="1" />
            <xsd:enumeration value="2" />
            <xsd:enumeration value="3" />
            <xsd:enumeration value="4" />
            <xsd:enumeration value="5" />
            <xsd:enumeration value="6" />
            <xsd:enumeration value="7" />
            <xsd:enumeration value="8" />
            <xsd:enumeration value="9" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="HeightIN"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            valid values are 0 through 11.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:int">
            <xsd:enumeration value="0" />
            <xsd:enumeration value="1" />
            <xsd:enumeration value="2" />
            <xsd:enumeration value="3" />
            <xsd:enumeration value="4" />
            <xsd:enumeration value="5" />
            <xsd:enumeration value="6" />
            <xsd:enumeration value="7" />
            <xsd:enumeration value="8" />
            <xsd:enumeration value="9" />
            <xsd:enumeration value="10" />
            <xsd:enumeration value="11" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Weight"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            valid values are 0 through 9999.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:int">
            <xsd:minInclusive value="0"/>
            <xsd:maxInclusive value="9999"/>
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="Smoker"
                   type="enumYesNos"
                   minOccurs="0" />
      <xsd:element name="MaritalStatus"
                   minOccurs="0">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Single" />
            <xsd:enumeration value="Married" />
            <xsd:enumeration value="Divorced" />
            <xsd:enumeration value="Separated" />
            <xsd:enumeration value="Widowed" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element name="ExpectantParent"
                   type="enumYesNos"
                   minOccurs="0" />
      <xsd:element name="CreditSelfRating"
                   minOccurs="0">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Poor" />
            <xsd:enumeration value="Average" />
            <xsd:enumeration value="Good" />
            <xsd:enumeration value="Excellent" />
            <xsd:enumeration value="Unsure" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>

      <xsd:element name="HazardousLifestyle"
                   type="enumYesNos"
                   minOccurs="0" />
      <xsd:element name="FamilyHistoryCancer"
                   type="enumYesNos"
                   minOccurs="0" />
      <xsd:element name="FamilyHistoryHeartDisease"
                   type="enumYesNos"
                   minOccurs="0" />

      <xsd:element name="DriverIncidents"
                   minOccurs="0">
        <xsd:complexType>
          <xsd:sequence maxOccurs="unbounded">
            <xsd:element name="Incident">
              <xsd:complexType>
                <xsd:all>
                  <xsd:element name="IncidentType">
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Ticket" />
                        <xsd:enumeration value="Accident" />
                        <xsd:enumeration value="DUI" />
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="IncidentDate"
                               type="xsd:date"
                               minOccurs="0"/>
                  <xsd:element name="TicketType"
                               minOccurs="0">
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Careless Driving" />
                        <xsd:enumeration value="Defective Equipment" />
                        <xsd:enumeration value="Failure to Obey Signal" />
                        <xsd:enumeration value="Speeding" />
                        <xsd:enumeration value="Other" />
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>

                  <xsd:element name="AccidentType"
                               minOccurs="0">
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Vehicle Hit Other Vehicle" />
                        <xsd:enumeration value="Vehicle Hit Pedestrian" />
                        <xsd:enumeration value="Vehicle Hit Property" />
                        <xsd:enumeration value="Vehicle Damaged Avoiding Accident" />
                        <xsd:enumeration value="Other Vehicle Hit Driver Vehicle" />
                        <xsd:enumeration value="Unlisted At Fault Accident" />
                        <xsd:enumeration value="Unlisted Not At Fault Accident" />
                        <xsd:enumeration value="Other" />
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="AccidentDamageType"
                               type="enumDamageTypes"
                               minOccurs="0" />
                  <xsd:element name="AccidentDriverAtFault"
                               type="enumYesNos"
                               minOccurs="0" />
                  <xsd:element name="AccidentClaimAmount"
                               minOccurs="0">
                    <xsd:annotation>
                      <xsd:documentation xml:lang="en">
                        valid values are 0 through 99999999.
                      </xsd:documentation>
                    </xsd:annotation>
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:int">
                        <xsd:minInclusive value="0"/>
                        <xsd:maxInclusive value="99999999"/>
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="DUIState_Abbr"
                               type="enumStates"
                               minOccurs="0" />
                  <xsd:element name="ShortDescription"
                               type="string50"
                               minOccurs="0" />
                </xsd:all>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>

      <xsd:element name="DriversLicense"
                   minOccurs="0">
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="State_Abbr"
                         type="enumStates">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  The State abbreviation that the license was issued
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="Number"
                         type="string50"
                         minOccurs="0"/>
            <xsd:element name="IssuedDate"
                         type="xsd:date"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  The format for the field is yyyy-MM-dd.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="Status"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Valid License"/>
                  <xsd:enumeration value="Suspended" />
                  <xsd:enumeration value="Probation" />
                  <xsd:enumeration value="Restricted" />
                  <xsd:enumeration value="Learner Permit" />
                  <xsd:enumeration value="Temporary" />
                  <xsd:enumeration value="International" />
                  <xsd:enumeration value="Expired" />
                  <xsd:enumeration value="Not Licensed"/>
                  <xsd:enumeration value="Other"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="EverSuspendedRevoked"
                         type="enumYesNos"
                         minOccurs="0"/>
            <xsd:element name="SR22Required"
                         type="enumYesNos"
                         minOccurs="0"/>
            <xsd:element name="DriverTraining"
                         type="enumYesNos"
                         minOccurs="0"/>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>

      <xsd:element name="CurrentInsurances"
                   minOccurs="0">
        <xsd:complexType>
          <xsd:sequence maxOccurs="unbounded">
            <xsd:element name="CurrentInsurance">
              <xsd:complexType>
                <xsd:all>
                  <xsd:element name="InsuranceCompany"
                               type="string250" />
                  <xsd:element name="PolicyExpirationDate"
                               type="xsd:date"
                               minOccurs="0" />
                  <xsd:element name="InsuranceCoveragePeriod"
                               minOccurs="0">
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="Less than 6 months" />
                        <xsd:enumeration value="More than 6 months" />
                        <xsd:enumeration value="More than 1 year" />
                        <xsd:enumeration value="More than 18 months" />
                        <xsd:enumeration value="More than 2 years" />
                        <xsd:enumeration value="More than 3 years" />
                        <xsd:enumeration value="More than 4 years" />
                        <xsd:enumeration value="More than 5 years" />
                        <xsd:enumeration value="More than 6 years" />
                        <xsd:enumeration value="More than 7 years" />
                        <xsd:enumeration value="More than 8 years" />
                        <xsd:enumeration value="More than 9 years" />
                        <xsd:enumeration value="More than 10 years" />
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                </xsd:all>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>

      <xsd:element name="CurrentMedicalConditions"
                   minOccurs="0">
        <xsd:complexType>
          <xsd:sequence maxOccurs="unbounded">
            <xsd:element name="CurrentMedicalCondition">
              <xsd:complexType>
                <xsd:all>
                  <xsd:element name="MedicalCondition">
                    <xsd:simpleType>
                      <xsd:restriction base="xsd:string">
                        <xsd:enumeration value="AIDS / HIV" />
                        <xsd:enumeration value="Alcohol / Drug Abuse" />
                        <xsd:enumeration value="Alzheimer's / Dementia" />
                        <xsd:enumeration value="Asthma" />
                        <xsd:enumeration value="Cancer" />
                        <xsd:enumeration value="High Cholesterol" />
                        <xsd:enumeration value="Clinical Depression" />
                        <xsd:enumeration value="Diabetes" />
                        <xsd:enumeration value="Heart Disease" />
                        <xsd:enumeration value="High Blood Pressure" />
                        <xsd:enumeration value="Hepatitis / Liver" />
                        <xsd:enumeration value="Kidney Disease" />
                        <xsd:enumeration value="Pulmonary Disease" />
                        <xsd:enumeration value="Stroke" />
                        <xsd:enumeration value="Ulcers" />
                        <xsd:enumeration value="Vascular Disease" />
                        <xsd:enumeration value="Other / Not Listed" />
                        <xsd:enumeration value="Heart Attack" />
                        <xsd:enumeration value="Emphysema" />
                        <xsd:enumeration value="Epilepsy" />
                        <xsd:enumeration value="Mental Illness" />
                        <xsd:enumeration value="Multiple Sclerosis" />
                      </xsd:restriction>
                    </xsd:simpleType>
                  </xsd:element>
                  <xsd:element name="MedicalOther"
                               type="string100"
                               minOccurs="0" />
                </xsd:all>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>

    </xsd:all>
    <xsd:attribute name="IndividualID"
                   type="xsd:int"
                   use="required" />
  </xsd:complexType>

  <!-- LeadOpportunitiesType -->
  <xsd:complexType name="LeadOpportunitiesType">
    <xsd:sequence maxOccurs="unbounded">
      <xsd:element name="Opportunity">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            We are planning on expanding this section in the near future.  It will be limited to only one for now.
          </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="InsuranceType"
                         type="enumInsuranceTypes" />
            <xsd:element name="RequestedEffectiveDate"
                         type="xsd:date"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  The format for the field is yyyy-MM-dd.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="RequestedCoverages"
                         minOccurs="0">
              <xsd:complexType>
                <xsd:sequence maxOccurs="unbounded">
                  <xsd:element name="RequestedCoverage">
                    <xsd:complexType>
                      <xsd:all>
                        <xsd:element name="CoverageType"
                                     type="enumCoverages" />
                        <xsd:element name="Qualifier"
                                     type="enumQualifiers"
                                     minOccurs="0" />
                        <xsd:element name="Value"
                                     type="string100"
                                     minOccurs="0"/>
                        <xsd:element name="Comment"
                                     type="xsd:string"
                                     minOccurs="0"/>
                      </xsd:all>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="RequestedAppointmentDT"
                         type="xsd:dateTime"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  The format for the field is yyyy-MM-ddTHH:mm:ssZ or yyyy-MM-ddTHH:mm:sszzz.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element name="GroupCompany"
                         minOccurs="0">
              <xsd:complexType>
                <xsd:all>
                  <xsd:element name="Name"
                               type="string75"/>
                </xsd:all>
              </xsd:complexType>
            </xsd:element>
            <xsd:element name="OpportunityStatus"
                         type="string50"
                         minOccurs="0" />

            <xsd:element name="LeadID"
                         type="validationGuidString"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  Identifier used by the LeadID lead assessment service to conduct risk assessment of leads.
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- LeadContactDetailsType -->
  <xsd:complexType name="LeadContactDetailsType">
    <xsd:all>
      <xsd:element name="Address"
                   type="AddressType"
                   minOccurs="0" />
      <xsd:element name="MailingAddress"
                   type="AddressType"
                   minOccurs="0" />
      <xsd:element name="PrimaryPhone"
                   type="validationPhone">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            The number being passed must be 10 digits in length that does not include the proceeding long distance code “1”.  Format: 9999999999
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="PrimaryPhoneExt"
                   type="string50"
                   minOccurs="0" />
      <xsd:element name="SecondaryPhone"
                   type="validationPhone"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            The number being passed must be 10 digits in length that does not include the proceeding long distance code “1”.  Format: 9999999999
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SecondaryPhoneExt"
                   type="string50"
                   minOccurs="0" />
      <xsd:element name="FaxPhone"
                   type="validationPhone"
                   minOccurs="0">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            The number being passed must be 10 digits in length that does not include the proceeding long distance code “1”.  Format: 9999999999
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="BestTimeToContact"
                   minOccurs="0">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="Morning" />
            <xsd:enumeration value="Afternoon" />
            <xsd:enumeration value="Evening" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <!-- Commented out on 8 Jan 2014 by JPZ; not sure we need this after all, but holding for further review-->
      <!--<xsd:element name="IsConsentToContact" type="enumYesNos" minOccurs="0">
        <xsd:annotation>
            <xsd:documentation xml:lang="en">
                Determines whether lead has given Consent To Contact.
                An update to the lead, if pre-existing, will only process if this flag is set to Yes; a No will be ignored by the system.
            </xsd:documentation>
        </xsd:annotation>
      </xsd:element>-->
    </xsd:all>
  </xsd:complexType>

  <!-- LeadVehiclesType -->
  <xsd:complexType name="LeadVehiclesType">
    <xsd:sequence maxOccurs="unbounded">
      <xsd:element name="Vehicle">
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="Year"
                         type="xsd:gYear"/>
            <xsd:element name="Make"
                         type="string100"/>
            <xsd:element name="Model"
                         type="string100"
                         minOccurs="0"/>
            <xsd:element name="Submodel"
                         type="string100"
                         minOccurs="0" />
            <xsd:element name="VIN"
                         type="string100"
                         minOccurs="0"/>

            <xsd:element name="VehicleUsage"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Commute" />
                  <xsd:enumeration value="Business" />
                  <xsd:enumeration value="Pleasure" />
                  <xsd:enumeration value="Farm/Ranch Use" />
                  <xsd:enumeration value="Government" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="AnnualMileage"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="0 - 5000" />
                  <xsd:enumeration value="5001 - 7500" />
                  <xsd:enumeration value="7501 - 10000" />
                  <xsd:enumeration value="10001 - 12500" />
                  <xsd:enumeration value="12501 - 15000" />
                  <xsd:enumeration value="15001 - 20000" />
                  <xsd:enumeration value="20001 - 25000" />
                  <xsd:enumeration value="25001 - 30000" />
                  <xsd:enumeration value="30001 - 40000" />
                  <xsd:enumeration value="40001 - 50000" />
                  <xsd:enumeration value="50001 and more" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="CollisionDeductible"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  No longer supported as of 20121116
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="No Coverage" />
                  <xsd:enumeration value="No Deductible" />
                  <xsd:enumeration value="50" />
                  <xsd:enumeration value="100" />
                  <xsd:enumeration value="150" />
                  <xsd:enumeration value="200" />
                  <xsd:enumeration value="250" />
                  <xsd:enumeration value="500" />
                  <xsd:enumeration value="750" />
                  <xsd:enumeration value="1000" />
                  <xsd:enumeration value="2500" />
                  <xsd:enumeration value="5000" />
                  <xsd:enumeration value="Other" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="ComprehensiveDeductible"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  No longer supported as of 20121116
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="No Coverage" />
                  <xsd:enumeration value="No Deductible" />
                  <xsd:enumeration value="50" />
                  <xsd:enumeration value="100" />
                  <xsd:enumeration value="150" />
                  <xsd:enumeration value="200" />
                  <xsd:enumeration value="250" />
                  <xsd:enumeration value="500" />
                  <xsd:enumeration value="750" />
                  <xsd:enumeration value="1000" />
                  <xsd:enumeration value="2500" />
                  <xsd:enumeration value="5000" />
                  <xsd:enumeration value="Other" />
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="Drivers"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  No longer supported as of 20121116
                  1. Each driver is a reference to the IndividualID.
                  2. The reference key can only be assigned once per vehicle.
                  3. Each vehicle must have a primary driver.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence maxOccurs="unbounded">
                  <xsd:element name="Driver">
                    <xsd:complexType>
                      <xsd:sequence>
                        <xsd:element name="PrimaryDriver"
                                     type="enumYesNos" />
                      </xsd:sequence>
                      <xsd:attribute name="IndividualIDRef"
                                     type="xsd:int"
                                     use="required"/>
                    </xsd:complexType>
                  </xsd:element>
                </xsd:sequence>
              </xsd:complexType>
              <xsd:unique name="unique2">
                <xsd:selector xpath="mstns:Driver"/>
                <xsd:field xpath="@IndividualIDRef"/>
              </xsd:unique>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- LeadBoatsType -->
