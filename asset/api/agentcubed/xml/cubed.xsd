  <!-- LeadHomesType -->
  <xsd:complexType name="LeadHomesType">
    <xsd:sequence maxOccurs="unbounded">
      <xsd:element name="Home">
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="Address"
                         type="AddressType"
                         minOccurs="0" />
            <xsd:element name="OccupiedByApplicant"
                         type="enumYesNos"
                         minOccurs="0"/>
            <xsd:element name="Bedrooms"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 9999.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="9999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="Bathrooms"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 9999.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="9999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="FireplacesOrWoodstoves"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 9999.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="9999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="YearBuilt"
                         type="xsd:gYear"
                         minOccurs="0" />
            <xsd:element name="DwellingType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Single Family Home"/>
                  <xsd:enumeration value="Multi Family Home"/>
                  <xsd:enumeration value="Apartment"/>
                  <xsd:enumeration value="Duplex"/>
                  <xsd:enumeration value="Condominium"/>
                  <xsd:enumeration value="Townhome"/>
                  <xsd:enumeration value="Mobile Home"/>
                  <xsd:enumeration value="Other"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="FoundationType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Basement 1/1 Finished"/>
                  <xsd:enumeration value="Basement 3/4 Finished"/>
                  <xsd:enumeration value="Basement 1/2 Finished"/>
                  <xsd:enumeration value="Basement 1/4 Finished"/>
                  <xsd:enumeration value="Basement Unfinished"/>
                  <xsd:enumeration value="Crawl Space"/>
                  <xsd:enumeration value="Piers Pilings Stilts"/>
                  <xsd:enumeration value="Slab"/>
                  <xsd:enumeration value="Other"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="FireExtinguisher"
                         type="enumYesNos"
                         minOccurs="0" />
            <xsd:element name="FireHydrantProximity"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Within 1000 Feet"/>
                  <xsd:enumeration value="Over 1000 Feet"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="FireStationProximity"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Within 5 Miles"/>
                  <xsd:enumeration value="Within 10 Miles"/>
                  <xsd:enumeration value="Over 10 Miles"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="FireAlarm"
                         type="enumYesNos"
                         minOccurs="0"/>
            <xsd:element name="SmokeDetector"
                         type="enumYesNos"
                         minOccurs="0"/>
            <xsd:element name="BurglarAlarm"
                         type="enumYesNos"
                         minOccurs="0"/>
            <xsd:element name="DesignType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="One Story"/>
                  <xsd:enumeration value="Bi-Level"/>
                  <xsd:enumeration value="Two Story"/>
                  <xsd:enumeration value="Tri-Level"/>
                  <xsd:enumeration value="Other"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="ExteriorWallType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Mostly Brick"/>
                  <xsd:enumeration value="Mostly Wood Frame"/>
                  <xsd:enumeration value="Mostly Stone"/>
                  <xsd:enumeration value="Aluminum Or Vinyl Siding"/>
                  <xsd:enumeration value="Veneer"/>
                  <xsd:enumeration value="Cinder Block"/>
                  <xsd:enumeration value="Stucco"/>
                  <xsd:enumeration value="Log"/>
                  <xsd:enumeration value="Other"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="ServicePanelType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Circuit Breaker"/>
                  <xsd:enumeration value="FuseBox"/>
                  <xsd:enumeration value="Other"/>
                  <xsd:enumeration value="Unknown"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="ConstructionType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Frame"/>
                  <xsd:enumeration value="Joisted Masonry"/>
                  <xsd:enumeration value="Masonry Non-combustible"/>
                  <xsd:enumeration value="Modified Resistive"/>
                  <xsd:enumeration value="Fire Resistive/Superior"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="GarageType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="One Car Attached"/>
                  <xsd:enumeration value="Two Car Attached"/>
                  <xsd:enumeration value="Three Car Attached"/>
                  <xsd:enumeration value="Carport Attached"/>
                  <xsd:enumeration value="One Car Detached"/>
                  <xsd:enumeration value="Two Car Detached"/>
                  <xsd:enumeration value="Three Car Detached"/>
                  <xsd:enumeration value="Carport Detached"/>
                  <xsd:enumeration value="Other"/>
                  <xsd:enumeration value="No Garage"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="AttackDog"
                         type="enumYesNos"
                         minOccurs="0"/>
            <xsd:element name="ResidenceType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Primary"/>
                  <xsd:enumeration value="Secondary"/>
                  <xsd:enumeration value="Prior"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="DeadBolt"
                         type="enumYesNos"
                         minOccurs="0"/>
            <xsd:element name="SquareFootage"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 999999.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="999999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="NumberStories"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="1-2"/>
                  <xsd:enumeration value="3-5"/>
                  <xsd:enumeration value="6-10"/>
                  <xsd:enumeration value="Over 10"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="UsedForBusinessOrFarming"
                         type="enumYesNos"
                         minOccurs="0"/>
            <xsd:element name="PropertyStatusType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Own"/>
                  <xsd:enumeration value="Rent"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="HeatingType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Gas Forced Air"/>
                  <xsd:enumeration value="Boiler"/>
                  <xsd:enumeration value="Oil Coal Kerosene"/>
                  <xsd:enumeration value="Propane"/>
                  <xsd:enumeration value="Electric"/>
                  <xsd:enumeration value="Stove"/>
                  <xsd:enumeration value="Other"/>
                  <xsd:enumeration value="Unknown"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="HeatingAge"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 999.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="RoofType"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="Composition Shingle"/>
                  <xsd:enumeration value="Wood Shingle"/>
                  <xsd:enumeration value="Tile"/>
                  <xsd:enumeration value="Concrete"/>
                  <xsd:enumeration value="Other"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="RoofAge"
                         minOccurs="0">
              <xsd:simpleType>
                <xsd:restriction base="xsd:string">
                  <xsd:enumeration value="1 To 5 Years"/>
                  <xsd:enumeration value="6 To 10 Years"/>
                  <xsd:enumeration value="11 Or More Years"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="PlumbingAge"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 999.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="ElectricAge"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 999.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="PropertyType"
                         type="enumPropertyTypes"
                         minOccurs="0"/>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- LeadSubsidiesType -->
  <xsd:complexType name="LeadSubsidiesType">
    <xsd:sequence maxOccurs="unbounded">
      <xsd:element name="Subsidy">
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="HouseholdIncome"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 9999999.99.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:minInclusive value="0.00"/>
                  <xsd:maxInclusive value="9999999.99"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="NumberOfAdults"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 99.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="99"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="NumberOfChildren"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 99.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="99"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="EstimatedMonthlyPremium"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 9999999.99.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:minInclusive value="0.00"/>
                  <xsd:maxInclusive value="9999999.99"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="EstimatedAnnualPremium"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 9999999.99.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:minInclusive value="0.00"/>
                  <xsd:maxInclusive value="9999999.99"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="MaximumPercentOfIncome"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 100.00.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:minInclusive value="0.00"/>
                  <xsd:maxInclusive value="100.00"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="CostShareReduction"
                         minOccurs="0">
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 9999999.99.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:decimal">
                  <xsd:minInclusive value="0.00"/>
                  <xsd:maxInclusive value="9999999.99"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- LeadVehicleLocationsType -->
  <xsd:complexType name="LeadVehicleLocationsType">
    <xsd:sequence maxOccurs="unbounded">
      <xsd:element name="VehicleLocation">
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="Address"
                         type="AddressType"
                         minOccurs="0" />
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- LeadInsuranceClaimsType -->
  <xsd:complexType name="LeadInsuranceClaimsType">
    <xsd:sequence maxOccurs="unbounded">
      <xsd:element name="InsuranceClaim">
        <xsd:complexType>
          <xsd:all>
            <xsd:element name="InsuranceType"
                         type="enumInsuranceTypes" />
            <xsd:element name="Date"
                         type="xsd:date"
                         minOccurs="0"/>
            <xsd:element name="DamageType"
                         type="enumDamageTypes"
                         minOccurs="0" />
            <xsd:element name="Amount"
                         minOccurs="0" >
              <xsd:annotation>
                <xsd:documentation xml:lang="en">
                  valid values are 0 through 99999999.
                </xsd:documentation>
              </xsd:annotation>
              <xsd:simpleType>
                <xsd:restriction base="xsd:int">
                  <xsd:minInclusive value="0"/>
                  <xsd:maxInclusive value="99999999"/>
                </xsd:restriction>
              </xsd:simpleType>
            </xsd:element>
            <xsd:element name="Comment"
                         type="xsd:string"
                         minOccurs="0"/>
          </xsd:all>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>

  <!-- AddressType -->
  <xsd:complexType name="AddressType">
    <xsd:all>
