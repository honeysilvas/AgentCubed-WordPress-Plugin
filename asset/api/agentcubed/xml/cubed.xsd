  <!-- Validation -->
  <xsd:simpleType name="validationGuidString">
    <xsd:restriction base="xsd:string">
      <xsd:pattern value="([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12})|(\{[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}\})"/>
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="validationZip">
    <xsd:restriction base="xsd:string">
      <xsd:length value="5" />
      <xsd:pattern value="(\d{5})" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="validationEmail">
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="100" />
      <xsd:pattern value="^$|[a-zA-Z_0-9\.\-]+@([a-zA-Z_0-9\-]+\.)+[a-zA-Z]{2,4}" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="validationPhone">
    <xsd:restriction base="xsd:string">
      <xsd:length value="10" />
      <xsd:pattern value="(\d{10})" />
    </xsd:restriction>
  </xsd:simpleType>

